{
  "name": "@enpage/sdk",
  "version": "0.0.10",
  "type": "module",
  "files": [
    "dist",
    "src",
    "env.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/sdk.js",
      "types": "./dist/sdk.d.ts"
    },
    "./env.d.ts": {
      "require": "./env.d.ts",
      "import": "./env.d.ts",
      "types": "./env.d.ts"
    },
    "./components": {
      "import": "./dist/components.js",
      "types": "./dist/components.d.ts"
    },
    "./datasources": {
      "import": "./dist/datasources.js",
      "types": "./dist/datasources.d.ts"
    },
    "./attributes": {
      "import": "./dist/attributes.js",
      "types": "./dist/attributes.d.ts"
    },
    "./context": {
      "import": "./dist/context.js",
      "types": "./dist/context.d.ts"
    },
    "./dynamic-css": {
      "import": "./dist/dynamic-css.js",
      "types": "./dist/dynamic-css.d.ts"
    },
    "./zod": {
      "import": "./dist/zod.js",
      "types": "./dist/zod.d.ts"
    }
  },
  "dependencies": {
    "@twind/core": "1.1.3",
    "clsx": "2.1.1",
    "immer": "10.1.1",
    "nanoid": "5.0.7",
    "tiny-invariant": "1.3.3",
    "zod": "workspace:*"
  },
  "peerDependencies": {
    "@enpage/style-system": "workspace:*",
    "@enpage/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "csstype": "3.1.3",
    "rollup-plugin-visualizer": "5.12.0",
    "vite": "5.2.11",
    "vite-plugin-dts": "workspace:*",
    "vite-tsconfig-paths": "workspace:*"
  },
  "scripts": {
    "build": "nx vite:build --verbose",
    "dev": "nx vite:build --watch",
    "lint": "npx nx eslint:lint",
    "preview": "nx vite:preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Matthias Etienne",
  "publishConfig": {
    "access": "public"
  }
}
